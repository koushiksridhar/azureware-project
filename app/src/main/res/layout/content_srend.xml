<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_srend"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/newbckg2"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.theparkcorp.azureware3.SREnd"
    tools:showIn="@layout/activity_srend">

    <TextView
        android:text="Results have been recorded"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:textStyle="bold"
        android:textSize="25sp"
        android:layout_marginTop="13dp"
        android:id="@+id/textView8"
        android:layout_below="@+id/textView7"
        android:layout_alignStart="@+id/textView7" />

    <TextView
        android:text="Today's survey is complete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/redTri3"
        android:textStyle="bold"
        android:textSize="25sp"
        android:layout_marginTop="126dp"
        android:id="@+id/textView7"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="End Survey"
        android:layout_width="200sp"
        android:layout_height="40sp"
        android:background="@drawable/rounded_button"
        android:textSize="20sp"
        android:textColor="@color/white"
        android:textStyle="bold"
        android:layout_marginTop="91dp"
        android:id="@+id/button5"
        android:onClick="endSurvey"
        android:layout_below="@+id/textView8"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
